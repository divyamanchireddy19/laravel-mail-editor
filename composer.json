{
    "name": "divyamanchireddy19/laravel-mail-editor",
    "description": "Laravel Mailable Editor",
    "license": "MIT",
    "authors": [
        {
            "name": "Div m",
            "email": "divyamanchireddy19@gmail.com",
            "homepage": "https://github.com/divyamanchireddy19"
        }
    ],
    "homepage": "https://github.com/divyamanchireddy19/laravel-mail-editor",
    "keywords": ["Laravel", "mailEclipse", "mailable-editor", "laravel-mail", "laravel-mailable", "mailable", "markdown"],
    "require": {
        "illuminate/support": "^8.0",
        "reecem/mocker": "^1.1",
        "laravel/legacy-factories": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "~1.2",
        "orchestra/testbench": "^6.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "divyamanchireddy19\\MailEclipse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "divyamanchireddy19\\MailEclipse\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "divyamanchireddy19\\MailEclipse\\MailEclipseServiceProvider"
            ],
            "aliases": {
                "MailEclipse": "divyamanchireddy19\\MailEclipse\\Facades\\MailEclipse"
            }
        }
    }
}
